apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/deploy-federation/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: admin 
  name: context
current-context: context
users:
- name: "admin"
  user:
    token: "<%= p("password") %>"
